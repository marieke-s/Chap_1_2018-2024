            # MOYENNE des mean
            files_mean = get_netcdf_paths_for_period(dt, base_folder, days=7, stat_type="mean")
            all_ws, all_vel = [], []
            for f in files_mean:
                ws_vals, vel_vals = get_ws_vel_for_poly(row['geometry'], f)
